	"authors": [
		"Qiu Tian"
	],
	"contact": {
		"sources": "https://github.com/qiutian-dev/fake-fps"
	},
	"license": "CC0-1.0",
	"icon": "assets/fake-fps/icon.png",
	"environment": "*",
